{
  "name": "plutus",
  "version": "1.0.0",
  "description": "Decentralized file storage on Ethereum",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "compile-contracts": "node scripts/compile",
    "deploy-contracts": "PROVIDER=8545 node scripts/deploy",
    "contract-example": "node scripts/example",
    "lint": "eslint ./ --ignore-path .gitignore",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "postinstall": "touch secrets.js",
    "test": "mocha",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & npm run start-server",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drummonda/plutus.git"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run lint-fix",
      "git add"
    ],
    "*.{css,scss,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Andrew Drummond",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drummonda/plutus/issues"
  },
  "homepage": "https://github.com/drummonda/plutus#readme",
  "dependencies": {
    "assert": "^1.4.1",
    "axios": "^0.18.0",
    "compression": "^1.7.3",
    "ethereumjs-util": "^5.2.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "ganache-cli": "^6.1.8",
    "history": "^4.7.2",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "openzeppelin-solidity": "^1.12.0",
    "pg": "^7.4.3",
    "react": "^16.5.2",
    "react-blockies": "^1.3.0",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^0.82.5",
    "sequelize": "^4.39.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "truffle-contract": "^3.0.6",
    "web3": "^1.0.0-beta.36",
    "webpack": "^4.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@svgr/cli": "^3.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-inline-react-svg": "^0.5.4",
    "eslint": "^5.6.1",
    "eslint-config-fullstack": "^6.0.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "forever-agent": "^0.6.1",
    "husky": "^1.1.0",
    "lint-staged": "^7.3.0",
    "nodemon": "^1.18.4",
    "npm-merge-driver": "^2.3.5",
    "path": "^0.12.7",
    "prettier": "^1.14.3",
    "redux-devtools-extension": "^2.13.5",
    "solc": "^0.4.25",
    "tough-cookie": "^2.4.3",
    "truffle-hdwallet-provider": "0.0.6",
    "tunnel-agent": "^0.6.0",
    "webpack-cli": "^3.1.2"
  }
}
